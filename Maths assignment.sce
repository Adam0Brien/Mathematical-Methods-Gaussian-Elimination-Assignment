function []= gaussian(a11,a12,a13,b1,a21,a22,a23,b2,a31,a32,a33,b3)  
m = [a11,a12,a13,b1;a21,a22,a23,b2;a31,a32,a33,b3]
disp(m)
m(2,:) = m(2,:)-m(2,1)*m(1,:)
disp(m)
m(3,:) = m(3,:)-m(3,1)*m(1,:) 
disp(m)
m(2,:) = m(2,:)/m(2,2)
disp(m)
m(3,:)=  m(3,:)- m(2,:)* m(3,2) 
disp(m)
m(3,:)= m(3,:)/m(3,3)
disp(m)
z = m(3,4)
y = -m(2,3)*z + m(2,4)
x =  -m(1,2)*y - m(1,3)*z + m(1,4)
disp('X = ')
disp(x)
disp('Y = ')
disp(y)
disp('Z = ')
disp(z)
endfunction
